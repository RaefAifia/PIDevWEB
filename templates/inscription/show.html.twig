{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>InscriptionId</th>
                <td>{{ inscription.inscriptionId }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ inscription.date ? inscription.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Isinscrit</th>
                <td>{{ inscription.isinscrit ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('inscription_index') }}">back to list</a>

    <a href="{{ path('inscription_edit', {'inscriptionId': inscription.inscriptionId}) }}">edit</a>

    {{ include('inscription/_delete_form.html.twig') }}
{% endblock %}
