{% extends 'base_back.html.twig' %}

{% block body %}
    <h1>Livraison</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>LivraisonId</th>
            <td>{{ livraison.livraisonId }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ livraison.nom }}</td>
        </tr>
        <tr>
            <th>Prenom</th>
            <td>{{ livraison.prenom }}</td>
        </tr>
        <tr>
            <th>NumTel</th>
            <td>{{ livraison.numTel }}</td>
        </tr>
        <tr>
            <th>Adresse</th>
            <td>{{ livraison.adresse }}</td>
        </tr>
        </tbody>
    </table>
    <center>

        <button style="margin-top:10px" class="btn btn-outline-primary"><a href="{{ path('livraison_indexadmin') }}">Retour</a></button>
        {{ include('livraison/_delete_form.html.twig') }}
    </center>
{% endblock %}
